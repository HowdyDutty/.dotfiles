[user]
	name = Roman Larionov
	email = romanylarionov@gmail.com
[color]
	ui = auto
[core]
	editor = vim
	autocrlf = input
[alias]
	unstage = reset HEAD
	unmerge = reset --merge
	cm = commit -m
	st = status
	br = branch
	co = checkout 
	pl = pull origin
	ps = push origin	
	up = !sh -c 'git pull --rebase --prune && git log --pretty=format:\"%Cred%ae %Creset- %C(yellow)%s %Creset(%ar)\" HEAD@{1}..'
[credential]
	helper = cache --timeout=9000
[help]
	autocorrect = 1
