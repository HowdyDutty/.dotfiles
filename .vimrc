
set nocompatible

set tabstop=4
set shiftwidth=4
set autoindent
set smarttab
set copyindent
set smartindent
set shiftround
set backspace=2

set ignorecase
set smartcase
set incsearch

syntax on
set number
set showmatch
set nobackup
set noswapfile
set term=xterm-256color
set colorcolumn=140
set cursorline
set visualbell
set t_vb=

let base16colorspace=256
set background=dark
